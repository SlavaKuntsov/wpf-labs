<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:vm="clr-namespace:_4.MVVM.ViewModel"
	xmlns:view="clr-namespace:_4.MVVM.View">

	<DataTemplate
		DataType="{x:Type vm:HomeViewModel}">
		<view:HomeView />
	</DataTemplate>

	<DataTemplate
		DataType="{x:Type vm:CatalogViewModel}">
		<view:CatalogView />
	</DataTemplate>

</ResourceDictionary>